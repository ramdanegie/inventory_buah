{
    "name": "laravel/lumen",
    "description": "The Laravel Lumen Framework.",
    "keywords": ["framework", "laravel", "lumen"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.1.3",
        "barryvdh/laravel-dompdf": "^0.8.3",
        "bschmitt/laravel-amqp": "2.*",
        "codedge/laravel-fpdf": "^1.2",
        "illuminate/mail": "^5.7",
        "jimmyjs/laravel-report-generator": "^1.1",
        "jimmyjs/pdf-report-generators": "^1.3",
        "laravel/lumen-framework": "5.7.*",
        "lcobucci/jwt": "^3.2",
        "mike42/escpos-php": "^2.1",
        "setasign/fpdf": "1.8.1",
        "tibonilab/pdf-lumen-bundle": "~2.0.0",
        "vlucas/phpdotenv": "~2.2",
        "webpatser/laravel-uuid": "2.*",
        "yectep/phpspreadsheet-bundle": "^0.0.5"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "phpunit/phpunit": "~7.0",
        "mockery/mockery": "~1.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/"
        ]
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
